{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --cache --ext .tsx,.ts,.js --fix",
    "format": "prettier . --write && yarn lint"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "17.0.19",
    "eslint": "^8.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
}
